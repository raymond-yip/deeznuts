{
    "environment": {
        "production": true,
        "name": "#{environmentName}",
        "apiUrl": "#{boomiApiUrl}",
        "okta": {
            "oidc": {
                    "issuer": "#{oktaIssuer}",
                    "redirectUri": "#{oktaRedirectUrl}",
                    "clientId": "#{oktaClientID}",
                    "scope": "openid profile email",
                    "testing": {
                        "disableHttpsCheck": false
                    }
            },
            "baseUrl": "#{oktaBaseUrl}",
            "apiKey": "#{oktaApiKey}"
        }
    }
}
